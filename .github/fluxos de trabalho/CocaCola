< roteiro >
    var  swiper  =  new  Swiper ( ".mySwiper" ,  {
      efeito : "fluxo de cobertura" ,
      grabCursor : true ,
      centeredSlides : true ,
      slidesPerView : "auto" ,
      efeito coverflow : {
        girar : 0 ,
        esticar : 0 ,
        profundidade : 200 ,
        modificador : 1 ,
        slideShadows : true ,
      } ,
      paginação : {
        el : ".swiper-pagination" ,
      } ,
      laço : verdadeiro ,
    } ) ;

    função  alternarMenu ( ) {
        const  menuToggle  =  document . querySelector ( '.toggle' ) ;
        const  navegação  =  documento . querySelector ( '.navigation' ) ;
        menuToggle . classList . alternar ( 'ativo' )
        navegação . classList . alternar ( 'ativo' )
    }
